{
    "name": "Tecno-Soluciones/wordbundle",
    "type": "symfony-bundle",
    "description": "This is a Symfony2 Bundle helps you to read and write Word files (including odt, rtf), thanks to the PHPWord library",
    "authors": [
        {
            "name": "Tecno-Soluciones",
            "email": "desarrollotecno@gmail.com"
        },
        {
            "name": "WordBundle Contributors",
            "homepage": "https://github.com/tecno/ExcelBundle#contributors"
        }
    ],

    "keywords": ["doc","Word", "symfony2", "bundle"],
    "homepage": "http://www.welcometothebundle.com",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "phpoffice/phpword": ">=0.10"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "symfony/finder": "~2.0",
        "symfony/form": "~2.1",
        "symfony/class-loader": "~2.1",
        "symfony/validator": "~2.1",
        "symfony/browser-kit": "~2.1",
        "sensio/framework-extra-bundle": "~2.1"
    },
	"repositories": [
    {
        "type": "package",
        "package": {
            "name": "tecno-soluciones/wordbundle",
            "version": "dev-master",
            "source": {
                "url": "git://github.com/tecno-soluciones/wordbundle.git",
                "type": "git",
                "reference": "master"
            },
            "autoload": {
                "psr-0" : {
                    "Tecno\\WordBundle" : "src"
                }
            }
        }
    }
],
    "autoload": {
        "psr-0": {
            "Tecno\\WordBundle": ""
        }
    },
    "target-dir": "Tecno-Soluciones/WordBundle",
    "config": {
        "bin-dir": "bin"
    }
}
